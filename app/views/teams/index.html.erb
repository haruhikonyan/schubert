<h1>団体一覧</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teams.each do |team| %>
      <tr>
        <td><%= team.name %></td>
        <td><%= link_to ' 詳細を表示', team %></td>
      </tr>
    <% end %>
  </tbody>
</table>

